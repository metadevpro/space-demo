    "mobx-react": "9.2.1",
    "nanoid": "5.1.6",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-icons": "5.5.0"
  },
  "devDependencies": {
    "@parcel/transformer-typescript-tsc": "2.16.0",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.1",
