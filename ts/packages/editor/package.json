{
  "name": "ts-space-demo-editor",
  "version": "0.0.1-alpha.0",
  "description": "Implementation of an editor (as Web app) for the space DSL",
  "private": true,
  "type": "module",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LionWeb-io/space-demo.git"
  },
  "bugs": {
    "url": "https://github.com/LionWeb-io/space-demo/issues"
